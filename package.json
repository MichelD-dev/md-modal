{
  "name": "@midly/react-modal",
  "version": "1.1.0",
  "description": "React-modal is a simplest way to create dialog on your ReactJS site.",
  "private": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc && tailwindcss build src/index.css -o dist/esm/index.css",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs && tailwindcss build src/index.css -o dist/cjs/index.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichelD-dev/react-modal.git"
  },
  "keywords": [
    "react",
    "react-component",
    "typescript",
    "modal",
    "dialog"
  ],
  "author": "MichelD-dev <delaunaymichel@hotmail.fr>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MichelD-dev/react-modal/issues"
  },
  "homepage": "https://github.com/MichelD-dev/react-modal#readme",
  "devDependencies": {
    "@types/react": ">=16",
    "@types/react-dom": ">=16",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
